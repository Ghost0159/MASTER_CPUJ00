==========IF FIRST/LAST===============
<IF.NITRO_LCFSPEC.GE.2>
---static---
<FOREACH.STATIC.OBJECTS><IF.STATIC.OBJECT.FIRST=   OBJS=:,><STATIC.OBJECT><IF.STATIC.OBJECT.LAST=;:><END.STATIC.OBJECTS>
<FOREACH.STATIC.LIBRARIES><IF.STATIC.LIBRARY.FIRST=   LIBS=:,><STATIC.LIBRARY><IF.STATIC.LIBRARY.LAST=;:><END.STATIC.LIBRARIES>
<FOREACH.STATIC.SEARCHSYMBOLS><IF.STATIC.SEARCHSYMBOL.FIRST=   SYMS=:,><STATIC.SEARCHSYMBOL><IF.STATIC.SEARCHSYMBOL.LAST=;:><END.STATIC.SEARCHSYMBOLS>

---autoload---
<FOREACH.AUTOLOADS><IF.AUTOLOAD.FIRST=ATLOADS=:,><AUTOLOAD.NAME><IF.AUTOLOAD.LAST=;:><END.AUTOLOADS>
<FOREACH.AUTOLOADS>

<AUTOLOAD.NAME>:
<FOREACH.AUTOLOAD.OBJECTS><IF.AUTOLOAD.OBJECT.FIRST=   OBJS=:,><AUTOLOAD.OBJECT><IF.AUTOLOAD.OBJECT.LAST=;:><END.AUTOLOAD.OBJECTS>
<FOREACH.AUTOLOAD.LIBRARIES><IF.AUTOLOAD.LIBRARY.FIRST=   LIBS=:,><AUTOLOAD.LIBRARY><IF.AUTOLOAD.LIBRARY.LAST=;:><END.AUTOLOAD.LIBRARIES>
<FOREACH.AUTOLOAD.SEARCHSYMBOLS><IF.AUTOLOAD.SEARCHSYMBOL.FIRST=   SYMS=:,><AUTOLOAD.SEARCHSYMBOL><IF.AUTOLOAD.SEARCHSYMBOL.LAST=;:><END.AUTOLOAD.SEARCHSYMBOLS>
<END.AUTOLOADS>

---overlay---
<FOREACH.OVERLAYS><IF.OVERLAY.FIRST=ATLOADS=:,><OVERLAY.NAME><IF.OVERLAY.LAST=;:><END.OVERLAYS>
<FOREACH.OVERLAYS>

<OVERLAY.NAME>:
<FOREACH.OVERLAY.OBJECTS><IF.OVERLAY.OBJECT.FIRST=   OBJS=:,><OVERLAY.OBJECT><IF.OVERLAY.OBJECT.LAST=;:><END.OVERLAY.OBJECTS>
<FOREACH.OVERLAY.LIBRARIES><IF.OVERLAY.LIBRARY.FIRST=   LIBS=:,><OVERLAY.LIBRARY><IF.OVERLAY.LIBRARY.LAST=;:><END.OVERLAY.LIBRARIES>
<FOREACH.OVERLAY.SEARCHSYMBOLS><IF.OVERLAY.SEARCHSYMBOL.FIRST=   SYMS=:,><OVERLAY.SEARCHSYMBOL><IF.OVERLAY.SEARCHSYMBOL.LAST=;:><END.OVERLAY.SEARCHSYMBOLS>
<END.OVERLAYS>
<ENDIF>


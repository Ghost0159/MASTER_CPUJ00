/********************************************************************/
ev_c07_flag_change:

	/*ギンガ　トバリ　ビルの前に国際警察をセット*/
	_IF_FLAGON_CALL	FE_C07_POLICE_WAIT,ev_c07_flag_change_police_set_02

	/*ジムクリア後のサポートイベント終了していたら*/
	_IFVAL_CALL		WK_SCENE_C07_SUPPORT_GYM_AFTER,GE,2,ev_c07_flag_change_support_set_02

	//WKOBJCHRにサポートのキャラコードをセットする
	_GET_MY_SEX		LOCALWORK0
	_IFVAL_JUMP		LOCALWORK0,EQ,0,c07_male_flag_change_01		//サポート女
	_IFVAL_JUMP		LOCALWORK0,EQ,1,c07_female_flag_change_01	//サポート男
	_END

/*ジムクリア後のサポートイベント終了していたら*/
ev_c07_flag_change_support_set_02:
	_SXY_POS_CHANGE	C07_SUPPORT,696,596
	_SXY_MV_CHANGE	C07_SUPPORT,MV_RIGHT
	_SXY_DIR_CHANGE	C07_SUPPORT,DIR_RIGHT
	_RET

c07_male_flag_change_01:	//サポート女
	_LDVAL			OBJCHRWORK0,HEROINE
	_END

c07_female_flag_change_01:	//サポート男
	_LDVAL			OBJCHRWORK0,HERO
	_END

/*ギンガ　トバリ　ビルの前に国際警察をセット*/
ev_c07_flag_change_police_set_02:
	_SXY_POS_CHANGE	C07_POLICE,717,593
	_SXY_MV_CHANGE	C07_POLICE,MV_UP
	_SXY_DIR_CHANGE	C07_POLICE,DIR_UP
	_RET



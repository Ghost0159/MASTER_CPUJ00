/*						ジムクリア前のサポート						*/
/********************************************************************/
#define POS_C07_SUPPORT_GYM_X1	(681)
#define POS_C07_SUPPORT_GYM_X2	(682)
#define POS_C07_SUPPORT_GYM_X3	(683)
#define POS_C07_SUPPORT_GYM_X4	(684)

pos_c07_support:
	_EVENT_START

	/*「！」*/
	_OBJ_ANIME		C07_SUPPORT,anm_c07_support_200
	_OBJ_ANIME_WAIT

	_CHG_COMMON_SCR SCRID_THE_BOY_GIRL_PLAY				/*サポート曲再生*/

	/*サポート近づく*/
	_PLAYER_POS_GET	SCWK_TEMP0,SCWK_TEMP1
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X1,ev_c07_support_gym_pos_01
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X2,ev_c07_support_gym_pos_02
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X3,ev_c07_support_gym_pos_03
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X4,ev_c07_support_gym_pos_04

	_GET_MY_SEX		SCWK_TEMP0
	_IFVAL_CALL		SCWK_TEMP0,EQ,0,c07_support_male_100
	_IFVAL_CALL		SCWK_TEMP0,EQ,1,c07_support_female_100
	_TALK_CLOSE
	_CHG_COMMON_SCR SCRID_THE_BOY_GIRL_STOP				/*サポート曲停止*/

	//ドアアニメをセットアップ
	_SET_UP_DOOR_ANIME	21,19,12,3,77

	//ドア開ける
	_SEQ_OPEN_DOOR	77

	//アニメ終了待ち
	_WAIT_3D_ANIME	77

	/*マキシマム仮面登場*/
	_FLAG_RESET		FV_C07_LEADER
	_OBJ_ADD		C07_LEADER
	_OBJ_ANIME		C07_LEADER,anm_c07_leader_210
	_OBJ_ANIME_WAIT

	//ドア閉める
	_SEQ_CLOSE_DOOR	77

	//アニメ終了待ち
	_WAIT_3D_ANIME	77

	//ドアアニメ解放
	_FREE_3D_ANIME	77

	/*上を見る*/
	_OBJ_ANIME		C07_SUPPORT,anm_c07_support_250
	//_OBJ_ANIME		FLDOBJ_ID_PLAYER,anm_c07_player_250
	_OBJ_ANIME_WAIT

	/*マキシマム仮面歌う*/
	_TALK_KEYWAIT	msg_c07_leader_01
	_TALK_CLOSE

	/*気づく*/
	_OBJ_ANIME		C07_LEADER,anm_c07_leader_520
	_OBJ_ANIME_WAIT

	/*マキシマム仮面近づく*/
	_PLAYER_POS_GET	SCWK_TEMP0,SCWK_TEMP1
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X1,ev_c07_support_gym_pos_11
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X2,ev_c07_support_gym_pos_12
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X3,ev_c07_support_gym_pos_13
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X4,ev_c07_support_gym_pos_14

	_TALK_KEYWAIT	msg_c07_leader_02

	_TALK_KEYWAIT	msg_c07_leader_03
	_TALK_CLOSE

	/*マキシマム仮面去っていく*/
	_PLAYER_POS_GET	SCWK_TEMP0,SCWK_TEMP1
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X1,ev_c07_support_gym_pos_21
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X2,ev_c07_support_gym_pos_22
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X3,ev_c07_support_gym_pos_23
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X4,ev_c07_support_gym_pos_24
	_OBJ_DEL		C07_LEADER

	_TIME_WAIT		20,SCWK_ANSWER

	/*画面下を見ながら*/
	_GET_MY_SEX		SCWK_TEMP0
	_IFVAL_CALL		SCWK_TEMP0,EQ,0,c07_support_male_110
	_IFVAL_CALL		SCWK_TEMP0,EQ,1,c07_support_female_110
	_TALK_CLOSE

	/*サポート去っていく*/
	_PLAYER_POS_GET	SCWK_TEMP0,SCWK_TEMP1
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X1,ev_c07_support_gym_pos_31
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X2,ev_c07_support_gym_pos_32
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X3,ev_c07_support_gym_pos_33
	_IFVAL_CALL		SCWK_TEMP0,EQ,POS_C07_SUPPORT_GYM_X4,ev_c07_support_gym_pos_34
	_OBJ_DEL		C07_SUPPORT

	_LDVAL			WK_SCENE_C07_SUPPORT,1
	_EVENT_END
	_END


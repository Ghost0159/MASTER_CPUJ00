	_LDVAL			SCWK_TEMP0,0			/*一人目*/

	/*すでに勝利しているかチェック*/
	_LDVAL			SCWK_TEMP2,0
	_IFVAL_CALL		OBJCHRWORK10,EQ,IDOL,ev_dosa_11_00
	_IFVAL_CALL		OBJCHRWORK10,EQ,REPORTER,ev_dosa_11_01
	//_IFVAL_CALL		OBJCHRWORK10,EQ,CAMERAMAN,ev_dosa_11_02		/*二人目しかいない*/

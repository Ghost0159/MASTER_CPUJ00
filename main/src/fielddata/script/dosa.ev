/********************************************************************/
ev_dosa_flag_change:

	_FLAG_SET	FV_DOSA_TR1
	_FLAG_SET	FV_DOSA_TR2

	/*バトルサーチャーを取得していなかったらキャラ表示しない*/
	_IF_FLAGOFF_JUMP SYS_FLAG_BS_LV1,ev_dosa_flag_change_end

	/*曜日によってセットされるキャラが変わる*/
	_CALL		ev_dosa_week
	_END

ev_dosa_flag_change_end:
	_END


